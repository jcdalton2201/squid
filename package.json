{
  "name": "@populus/squid",
  "version": "0.0.1",
  "description": "Component Library for FED at Populus Fin",
  "main": "index.js",
  "files": ["dist"],
  "scripts": {
    "bs": "browser-sync start -c ./bs-config.js",
    "build": "rollup -c",
    "build-storybook": "build-storybook",
    "cc": "node scripts/createComponent.js",
    "coverage": "nyc --reporter=lcov --reporter=text jasmine jasmine spec/**/*.js",
    "docs": "build-storybook -o ./docs",
    "lint": "eslint src --ignore-pattern *.stories.* --fix",
    "postcc": "npm run lint",
    "prebuild": "node scripts/buildfile.js && npm run lint",
    "prestart": "npm run build",
    "pretest": "npm run build",
    "prework": "npm run build",
    "storybook": "start-storybook -p 6006",
    "test": "jasmine spec/**/*.js",
    "prewatchFiles": "npm run lint",
    "watchFiles": "rollup -c",
    "start": "npm-run-all -p bs watch",
    "watch": "watch \"npm run watchFiles\" src/"
  },
  "watch": {
    "start": "src/**/*.js"
  },
  "author": "Jason Dalton",
  "license": "MIT",
  "devDependencies": {
    "@axe-core/puppeteer": "^4.1.0",
    "@babel/core": "^7.12.8",
    "@storybook/addon-a11y": "^6.1.11",
    "@storybook/addon-actions": "^6.1.5",
    "@storybook/addon-essentials": "^6.1.5",
    "@storybook/addon-knobs": "^6.1.5",
    "@storybook/addon-links": "^6.1.5",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/html": "^6.1.5",
    "@storybook/web-components": "^6.1.5",
    "babel-loader": "^8.2.1",
    "browser-sync": "^2.26.13",
    "chalk": "^4.1.0",
    "differencify": "^1.5.5",
    "eslint": "^7.14.0",
    "eslint-plugin-jasmine": "^4.1.1",
    "eslint-plugin-prettier": "^3.1.4",
    "globby": "^11.0.1",
    "jasmine": "^3.6.3",
    "jasmine-core": "^3.6.0",
    "line-column": "^1.0.2",
    "lit-html": "^1.3.0",
    "minimist": "^1.2.5",
    "mkdirp": "^1.0.4",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.2.0",
    "puppeteer": "^5.5.0",
    "puppeteer-to-istanbul": "^1.4.0",
    "react-is": "^17.0.1",
    "rollup": "^2.33.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-postcss-lit": "^1.0.1",
    "source-map": "^0.7.3",
    "watch": "^1.0.2"
  },
  "nyc": {
    "exclude": [
      "spec"
    ],
    "ignore": [
      "utils/standardize-key-code.js"
    ]
  },
  "dependencies": {
    "lit-element": "^2.4.0"
  }
}
